{
    "swagger": "2.0",
    "info": {
        "description": "This is a sample serice for music app",
        "title": "Music API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "email": "soberkoder@swagger.io"
